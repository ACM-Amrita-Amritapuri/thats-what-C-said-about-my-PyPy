public class Pattern
{
    public static void main(String[] args)
    {
        int sp=10;
        for (int i=1;i<=5;i++)
        {
            for (int j=1; j<=i;j++)
            {
                System.out.print("");
                
            }
            for(int j=1;j<=i;j++)
            {
                System.out.print(i+"");
            }
        System.out.println();
        sp--;
        }
        
    }
}
